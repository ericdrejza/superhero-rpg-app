{
  "name": "superhero-rpg-app",
  "version": "1.0.0",
  "description": "Companion app for my superhero RPG",
  "scripts": {
    "server": "npm run --prefix server start",
    "dev-server": "npm run --prefix server dev",
    "client": "npm run --prefix client start",
    "dev": "concurrently \"npm run dev-server\" \"npm run client\"",
    "install-all": "cd client && npm install && cd - && cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericdrejza/superhero-rpg-app.git"
  },
  "keywords": [],
  "author": "",
