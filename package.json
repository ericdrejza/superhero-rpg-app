  "bugs": {
    "url": "https://github.com/ericdrejza/superhero-rpg-app/issues"
  },
  "homepage": "https://github.com/ericdrejza/superhero-rpg-app#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
